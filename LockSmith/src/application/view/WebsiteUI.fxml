<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#585e65" height="400.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
    <Rectangle fx:id="websiteDisplayField" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="40.0" layoutY="53.0" stroke="BLACK" strokeType="INSIDE" width="300.0" />
    <Label layoutX="40.0" layoutY="185.0" text="Username">
      <font>
        <Font name="System Bold" size="20.0" fx:id="x1" />
      </font>
      <textFill>
        <Color blue="0.988" green="0.988" red="0.988" fx:id="x2" />
      </textFill>
    </Label>
    <Label font="$x1" layoutX="45.0" layoutY="235.0" text="Password" textFill="$x2" />
    <Label font="$x1" layoutX="83.0" layoutY="286.0" text="Email" textFill="$x2" />
    <ImageView fx:id="websiteLogo" fitHeight="125.0" fitWidth="125.0" layoutX="421.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" />
    <TextField fx:id="usernameField" layoutX="166.0" layoutY="188.0" prefHeight="25.0" prefWidth="330.0" />
    <TextField fx:id="passwordField" layoutX="166.0" layoutY="238.0" prefHeight="25.0" prefWidth="330.0" />
    <TextField fx:id="emailField" layoutX="166.0" layoutY="288.0" prefHeight="25.0" prefWidth="330.0" />
    <Button fx:id="usernameCopy" layoutX="520.0" layoutY="184.0" mnemonicParsing="false">
      <graphic>
        <ImageView fitHeight="25.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@copyIcon.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <Button fx:id="passwordCopy" layoutX="520.0" layoutY="234.0" mnemonicParsing="false">
      <graphic>
        <ImageView fitHeight="25.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@copyIcon.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <Button fx:id="emailCopy" layoutX="520.0" layoutY="284.0" mnemonicParsing="false">
      <graphic>
        <ImageView fitHeight="25.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@copyIcon.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <BorderPane layoutX="40.0" layoutY="53.0" prefHeight="50.0" prefWidth="300.0">
      <center>
        <TextField fx:id="websiteName" alignment="CENTER" prefWidth="200.0" promptText="Insert Website Name" text="&lt;Insert Website Name&gt;" BorderPane.alignment="CENTER" />
      </center>
    </BorderPane>
    <Button fx:id="saveButton" font="$x1" layoutX="295.0" layoutY="332.0" mnemonicParsing="false" onAction="#save" text="Save" />
  </children>
</AnchorPane>
